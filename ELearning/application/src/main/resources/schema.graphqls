type Query {
    users: [User]
    userByUsername(username: String): User
}

type User {
	id: ID!
	firstName: String,
	middleName: String,
	lastName: String,
	age: Int,
	gender: String,
	username: String,
	password: String,
	createdOn: Float,
	modifiedOn: Float,
	lastLoginTime: Float,
	contact: Contact,
	type: String
}

type Contact {
	id: ID,
	userId: Float,
	email: String,
	mobileNumber: Float,
	address: String
}